{
  "name": "@template-tools/sync-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "cli for @template-tools/template-sync",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "template-sync": "./src/template-sync-cli.mjs"
  },
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp"
  },
  "dependencies": {
    "@template-tools/template-sync": "^11.0.19",
    "aggregation-repository-provider": "^5.2.11",
    "bitbucket-repository-provider": "^4.1.19",
    "commander": "^9.2.0",
    "gitea-repository-provider": "^2.1.40",
    "github-repository-provider": "^7.26.28",
    "local-repository-provider": "^8.0.6",
    "remove-sensible-values": "^1.0.13"
  },
  "devDependencies": {
    "ava": "^4.2.0",
    "c8": "^7.11.2",
    "execa": "^6.1.0",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/template-tools/template-sync-cli.git"
  },
  "bugs": {
    "url": "https://github.com/template-tools/template-sync-cli/issues"
  },
  "homepage": "https://github.com/template-tools/template-sync-cli#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-node-app"
    ]
  }
}
